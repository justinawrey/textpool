{
    "name": "textpool",
    "version": "0.1.0",
    "description": "spotify web queue with sms",
    "main": "index.js",
    "scripts": {
        "server-dev": "nodemon --exec babel-node server/index.js",
        "server-prod": "node ./build/index.js",
        "install-all": "npm i && cd client && npm i && cd ..",
        "start": "concurrently \"nodemon --exec babel-node server/index.js\" \"cd client && npm start\"",
        "clean": "rm -rf build && mkdir build",
        "postinstall": "npm run clean && npm run build-server",
        "build-server": "babel server --out-dir build",
        "heroku-prebuild": "cd client && npm i && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/justinawrey/textpool.git"
    },
    "keywords": [
        "sms",
        "web",
        "spotify",
        "music",
        "playlist",
        "queue",
        "text"
    ],
    "author": "Justin Awrey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/justinawrey/textpool/issues"
    },
    "homepage": "https://github.com/justinawrey/textpool#readme",
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "express-ws": "^4.0.0",
        "morgan": "^1.9.0",
        "socket.io": "^2.1.1",
        "spotify-web-api-node": "^4.0.0",
        "twilio": "^3.19.2",
        "uuid": "^3.3.2"
    },
    "browserslist": "> 0.25%, not dead",
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "concurrently": "^3.6.1",
        "dotenv": "^6.0.0",
        "nodemon": "^1.18.3"
    }
}
