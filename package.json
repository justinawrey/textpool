{
    "name": "textpool",
    "version": "0.1.0",
    "description": "spotify web queue with sms",
    "main": "index.js",
    "scripts": {
        "server": "nodemon --exec babel-node server/index.js",
        "install-all": "npm i && cd client && npm i && cd ..",
        "start": "concurrently \"nodemon --exec babel-node server/index.js\" \"cd client && npm start\"",
        "heroku": "NODE_ENV=production npm run install-all && cd client && npm run build && cd .. && npm run server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/justinawrey/textpool.git"
    },
    "keywords": [
        "sms",
        "web",
        "spotify",
        "music",
        "playlist",
        "queue",
        "text"
    ],
    "author": "Justin Awrey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/justinawrey/textpool/issues"
    },
    "homepage": "https://github.com/justinawrey/textpool#readme",
    "dependencies": {
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "express-ws": "^4.0.0",
        "morgan": "^1.9.0",
        "socket.io": "^2.1.1",
        "spotify-web-api-node": "^3.1.1",
        "twilio": "^3.19.2",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^3.6.1",
        "dotenv": "^6.0.0",
        "nodemon": "^1.18.3"
    }
}
